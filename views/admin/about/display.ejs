<%- include('../../header.ejs')%>
  <section class="dashboard-wrap mtb-40">
    <div class="container">
      <div class="body-content">
        <div class="row">
          <div class="col-md-3">
            <div class="dash-left">
              <ul>
                <li class="active"><a href="/admin/dashboard">Dashboard</a></li>
                <li><a href="/admin/blogdisplay" class="active">Blogs</a></li>
                <li><a href="/admin/category">Categories</a></li>
                <li><a href="/admin/contact">Contact</a></li>
                <li><a href="/admin/about">About</a></li>
                <li><a href="/logout">Logout</a></li>
              </ul>
            </div>
          </div>
          <div class="col-md-9">
            <div class="dash-right">
              <div class="dash-header">
                <div class="dash-title">
                  <h1>About</h1>
                </div>
              </div>
              <table class="table table-bordered table-striped">
                <tr>

                  <th style="color: aliceblue;">Content</th>
                  <th style="color: aliceblue;">Action</th>
                </tr>


                <% c.forEach((b)=>{ %>
                  <tr>

                    <td style="color: aliceblue;">
                      <%= b.content %>
                    </td>
                    <td>
                      <a href="/admin/aboutedit" class="btn btn-warning"><i class="fa-regular fa-pen-to-square"></i></a>
                    </td>

                    <% }) %>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <%- include('../../footer.ejs')%>