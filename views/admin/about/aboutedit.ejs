<%- include('../../header.ejs')%>
    <section class="dashboard-wrap mtb-40">
        <div class="container">
            <div class="body-content">
                <div class="row">
                    <div class="col-md-3">
                        <div class="dash-left">
                            <ul>
                                <li class="active"><a href="/admin/dashboard">Dashboard</a></li>
                                <li><a href="/admin/display" class="active">Blogs</a></li>
                                <li><a href="/admin/category">Categories</a></li>
                                <li><a href="/admin/contactdisplay">Contact</a></li>
                                <li><a href="/admin/about">About</a></li>
                                <li><a href="/logout">Logout</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-9">
                        <div class="dash-right">
                            <div class="dash-header">
                                <div class="dash-title">
                                    <h1>About
                                    </h1>
                                </div>
                            </div>
                            <form method="post" action="/aboutupdate" enctype="multipart/form-data">

                                <div class="form-group form-row">
                                    <label class="col-md-3 required"></label>
                                    <div class="col-md-9">
                                        <input type="text" value="<%= c.content %>" class="form-control" name="content"
                                            placeholder="content" required />
                                    </div>
                                </div>

                                <div class="form-group form-row">
                                    <label class="col-md-3"></label>
                                    <div class="col-md-9">
                                        <button type="submit" class="btn btn-primary">Update</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <%- include('../../footer.ejs')%>